{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^3.10|^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^3.3|^4.0",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "blackfire/php-sdk": "^2.5",
        "darkaonline/l5-swagger": "^9.0",
        "deptrac/deptrac": "^4.1",
        "enlightn/security-checker": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "infection/infection": "^0.31.2",
        "itsgoingd/clockwork": "^5.3",
        "larastan/larastan": "^2.11|^3.0",
        "laravel/dusk": "^8.3",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nikic/php-parser": "^5.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "nunomaduro/phpinsights": "^2.13",
        "phpmd/phpmd": "^2.15",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.1",
        "roave/security-advisories": "dev-latest",
        "sentry/sentry-laravel": "^4.15",
        "spatie/laravel-ignition": "^2.7",
        "vimeo/psalm": "^6.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize",
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi --quiet",
            "@php artisan view:cache",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan storage:link --quiet"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize",
            "@php artisan view:cache",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "pre-commit": [
            "@php artisan test",
            "@php ./vendor/bin/phpstan analyse",
            "@php ./vendor/bin/phpmd app text cleancode,codesize,controversial,design,naming,unusedcode",
            "@php ./vendor/bin/pint --test"
        ],
        "format": [
            "@php ./vendor/bin/php-cs-fixer fix"
        ],
        "analyse": [
            "@php ./vendor/bin/phpstan analyse",
            "@php ./vendor/bin/phpmd app text cleancode,codesize,controversial,design,naming,unusedcode",
            "@php artisan test --parallel"
        ],
        "clear-all": [
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "composer dump-autoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan view:cache"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan config:clear",
            "@php artisan test --without-brave"
        ],
        "test:coverage": [
            "@php artisan test --coverage"
        ],
        "test:dusk": [
            "@php artisan dusk"
        ],
        "test:infection": [
            "infection --threads=max"
        ],
        "test:all": [
            "@test",
            "@test:dusk",
            "@test:infection"
        ],
        "analyse:phpstan": [
            "php -d memory_limit=1G ./vendor/bin/phpstan analyse"
        ],
        "analyse:psalm": [
            "./vendor/bin/psalm"
        ],
        "analyse:insights": [
            "./vendor/bin/phpinsights analyse app"
        ],
        "analyse:security": [
            "composer audit",
            "./vendor/bin/security-checker security:check"
        ],
        "analyse:all": [
            "@analyse:phpstan",
            "@analyse:psalm",
            "@analyse:insights",
            "@analyse:security"
        ],
        "fix:style": [
            "./vendor/bin/php-cs-fixer fix"
        ],
        "fix:rector": [
            "./vendor/bin/rector process app"
        ],
        "fix:all": [
            "@fix:style",
            "@fix:rector"
        ],
        "metrics": [
            "./vendor/bin/phpmetrics --config=phpmetrics.json app"
        ],
        "quality": [
            "@fix:all",
            "@analyse:all",
            "@test:all"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
