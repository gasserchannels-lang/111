{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "darkaonline/l5-swagger": "^9.0",
        "darryldecode/cart": "^4.2",
        "doctrine/dbal": "^4.3",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/cashier": "*",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^3.3|^4.0",
        "laravel/tinker": "^2.8",
        "livewire/livewire": "^3.0",
        "srmklive/paypal": "*",
        "stripe/stripe-php": "*"
    },
    "require-dev": {
        "enlightn/security-checker": "^2.0",
        "fakerphp/faker": "*",
        "laravel/pint": "^1.25",
        "mockery/mockery": "*",
        "larastan/larastan": "^3.7",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.0",
        "vimeo/psalm": "^6.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "classmap": [
            "tests/",
            "tests/AI"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-install-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize",
            "php -r \"file_exists('.env' ) || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi --quiet",
            "@php artisan view:cache",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan storage:link --quiet"
        ],
        "post-update-cmd": [
            "@php artisan clear-compiled",
            "@php artisan optimize",
            "@php artisan view:cache",
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "pre-commit": [
            "vendor/bin/phpunit",
            "@php ./vendor/bin/phpstan analyse",
            "@php ./vendor/bin/phpmd app text cleancode,codesize,controversial,design,naming,unusedcode",
            "@php ./vendor/bin/pint --test"
        ],
        "format": [
            "@php ./vendor/bin/php-cs-fixer fix"
        ],
        "analyse": [
            "@php ./vendor/bin/phpstan analyse",
            "@php ./vendor/bin/phpmd app text cleancode,codesize,controversial,design,naming,unusedcode",
            "vendor/bin/phpunit --parallel"
        ],
        "clear-all": [
            "@php artisan cache:clear",
            "@php artisan config:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "composer dump-autoload -o"
        ],
        "cache-all": [
            "@php artisan config:cache",
            "@php artisan route:cache",
            "@php artisan view:cache"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": [
            "@php artisan config:clear",
            "vendor/bin/phpunit"
        ],
        "test:coverage": [
            "vendor/bin/phpunit --coverage-html storage/logs/coverage"
        ],
        "test:dusk": [
            "@php artisan dusk"
        ],
        "test:infection": [
            "infection --threads=max"
        ],
        "test:all": [
            "vendor/bin/phpunit",
            "@test:dusk",
            "@test:infection"
        ],
        "analyse:phpstan": [
            "php -d memory_limit=1G ./vendor/bin/phpstan analyse"
        ],
        "analyse:psalm": [
            "./vendor/bin/psalm"
        ],
        "analyse:insights": [
            "./vendor/bin/phpinsights analyse app"
        ],
        "analyse:security": [
            "composer audit",
            "./vendor/bin/security-checker security:check"
        ],
        "analyse:all": [
            "@analyse:phpstan",
            "@analyse:psalm",
            "@analyse:insights",
            "@analyse:security"
        ],
        "fix:style": [
            "./vendor/bin/php-cs-fixer fix"
        ],
        "fix:rector": [
            "./vendor/bin/rector process app"
        ],
        "fix:all": [
            "@fix:style",
            "@fix:rector"
        ],
        "metrics": [
            "./vendor/bin/phpmetrics --config=phpmetrics.json app"
        ],
        "quality": [
            "@fix:all",
            "@analyse:all",
            "@test:all"
        ],
        "test:ai": [
            "vendor/bin/phpunit --testsuite AI"
        ],
        "test:security": [
            "vendor/bin/phpunit --testsuite Security"
        ],
        "test:performance": [
            "vendor/bin/phpunit --testsuite Performance"
        ],
        "test:integration": [
            "vendor/bin/phpunit --testsuite Integration"
        ],
        "test:comprehensive": [
            "vendor/bin/phpunit --configuration=phpunit.xml --log-junit=storage/logs/junit.xml --coverage-html=storage/logs/coverage"
        ],
        "test:30": [
            "@test:ai",
            "@test:security",
            "@test:performance",
            "@test:integration",
            "@test:comprehensive"
        ],
        "measure:all": [
            "./vendor/bin/pint --test",
            "./vendor/bin/phpstan analyse --memory-limit=1G",
            "./vendor/bin/phpmd app text cleancode,codesize,controversial,design,naming,unusedcode",
            "composer audit",
            "@test:comprehensive"
        ],
        "cleanup": [
            "php cleanup-environment.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "spatie/laravel-ignition"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "process-timeout": 600
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
