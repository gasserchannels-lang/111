includes:
    - ./vendor/larastan/larastan/extension.neon
    - ./phpstan-baseline.neon

parameters:
    level: 9
    paths:
        - app/
        - config/
        - database/
        - routes/
    tmpDir: storage/framework/cache/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    reportUnmatchedIgnoredErrors: true
    parallel:
        maximumNumberOfProcesses: 4
        processTimeout: 300.0
    excludePaths:
        - tests/*
        - storage/
        - bootstrap/cache/
    ignoreErrors:
        - '#Template type TDeclaringModel on class.*is not covariant#'
        - '#Template type TValue on class.*is not covariant#'
        - '#Template type TValue on class.*Collection.*is not covariant#'
        - '#Template type TValue on class.*Support.*Collection.*is not covariant#'
        - '#Template type TValue on class Illuminate\\\\Support\\\\Collection is not covariant#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more#'
        - '#Template type TValue on class Illuminate\\Support\\Collection is not covariant#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at https://phpstan.org#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at https://phpstan.org/blog#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at https://phpstan.org/blog/whats-up-with-template-covariant#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at https://phpstan.org/blog/whats-up-with-template-covariant#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at https://phpstan.org/blog/whats-up-with-template-covariant#'
        - '#Template type TValue on class.*Collection.*is not covariant.*Learn more at https://phpstan.org/blog/whats-up-with-template-covariant#'
